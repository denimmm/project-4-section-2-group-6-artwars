<Window x:Class="ArtWarsClientWPF.ResultWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ArtWarsClientWPF"
        mc:Ignorable="d"
        Title="ResultWindow" Height="450" Width="800">
    <Grid>
        <TextBlock Text="Winner:" TextAlignment="Center" FontSize="40" />
        <Image x:Name="WinnerImage" Height="300" Stretch="Uniform" Margin="191,67,191,67"/>
        <Button x:Name="CloseButton" Content="Close" HorizontalAlignment="Right" VerticalAlignment="Top" Width="100" Height="30" Margin="10,10,10,0" Click="CloseButton_Click"/>
    </Grid>
</Window>
